<mvc:View
    controllerName="com.te.project1.controller.covid.CovidList"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:l="sap.ui.layout"
    xmlns:core="sap.ui.core"
    xmlns:f="sap.f"
    xmlns="sap.m"
>
    <Page
        id="covidList"
        showHeader="Latest Covid Data"
        class="sapUiContentPadding"
    >

    	<List
        headerText="Total Cases : {Latest>/data/summary/total}"
        footertext=" Latest Covid count : "
		id="myList"
		items="{Latest>/data/regional}">
		<items>
			<StandardListItem
				title="{Latest>loc}"
				description="Total Cases : {Latest>totalConfirmed}"
				info="Deaths : {Latest>deaths}"
				 />
		</items>
	</List>
       

    </Page>
</mvc:View>
